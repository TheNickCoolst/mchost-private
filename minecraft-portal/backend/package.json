{
  "name": "minecraft-portal-backend",
  "version": "1.0.0",
  "description": "Backend API for Minecraft server management portal",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "seed": "ts-node src/seeds/seed-minecraft-data.ts",
    "seed:prod": "node dist/seeds/seed-minecraft-data.js",
    "seed:plugins": "ts-node src/seeds/seed-plugins.ts",
    "seed:all": "npm run seed && npm run seed:plugins"
  },
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.14",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "https": "^1.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "minecraft",
    "server",
    "management",
    "api"
  ],
  "author": "In-House Development Team",
  "license": "MIT"
}